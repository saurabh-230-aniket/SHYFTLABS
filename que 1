#include <bits/stdc++.h>
using namespace std;

int main() 
{
    vector<int>vec;
    int n;
    cin>>n;
    for(int i=0;i<n;i++)
    {
      int element;
      cin>>element;
      vec.push_back(element);
    }
    for(int i=0;i<n;i++)
    {
      if(vec[i]==-1)
      {
        cout<<i<<endl;
      }
    }
    return 0;
}
